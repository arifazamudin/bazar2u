    <section class="site-section bg-light">
      <div class="container">

        <h1 class="mb-4">Order Summary</h1>
        <div class="row">
          <div class="col-md-12">
            <h5> Order ID : bzr#<%= newID%></h5>
            
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <label><b>Name</b></label>
            <p><%= name %></p>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <label><b>Address</b></label>
            <p><%= address %></p>
          </div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <label><b>Phone #1</b></label>
            <p><%= phone1 %></p>
          </div>
          <div class="col-md-6">
            <label><b>Phone #2</b></label>
            <p><%= phone2 %></p>
          </div>
        </div>


        <br><br>
                <% if (typeof products !== "undefined") { %>
                <table class="table table-striped alignmiddle">
                    
                    <tr> 
                        <th>Image</th>
                        <th>Title</th>
                        <th>Price</th>
                        <th>Quantity</th>
                        <th align="right">Subtotal</th>
                    </tr>
                    
                    <% var total = 0; %>
                    
                    <% products.forEach(function(product){ %>
                    <% var sub = parseFloat(product.qty * product.price).toFixed(2) %>
                    <% total += +sub %>
                    
                    <tr>
                        <td><img class="cpi" src="https://bazar2you.s3-ap-southeast-1.amazonaws.com<%= product.image %>" alt=""></td>
                        <td><%= product.title %></td>
                        <td>RM<%= product.price %></td>
                        <td><%= product.qty %></td>
                        <td align="right">RM<%= sub %></td>
                    </tr>
                    <% }); %>
                    <tr>
                        <td colspan="5" align="right"><b>Sub Total: </b> RM  <%= parseFloat(total).toFixed(2) %></td>
                        
                    </tr>
                    <tr>
                      <td colspan="5" align="right"><b>Delivery: </b> RM  4</td>
                    </tr>
                    <tr>
                      <td colspan="5" align="right"><b>Total: </b> RM  <%= parseFloat(total+4).toFixed(2) %></td>
                    </tr>
                    <% } %>
                </table>
      </div>



