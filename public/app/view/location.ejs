            style="width: 100%; height: 200px;border-radius: 20px;">
            </div> -->
            <br>
            <div >
                <h2 for="">Pilih lokasi anda</h2>
                <% checkpoints.forEach(function(c){ %>
                  <% if (c.title === "Kempas"|| c.title === "Taman Perling"|| c.title === "Mutiara Rini") {%>
                    <li class="list-group-item"><a href="/location"><%= c.title %> (Coming Soon)</a></li>
                    <% } else {%>
                    <li class="list-group-item"><a href="/products/<%= c.slug %>"><%= c.title %></a></li>
                    <% } %>
                    <% }); %>
            </div>
            <br>
          <div>
            <p>Klik <u><a href="https://wasap.my/60187700704"> sini</a></u> untuk sebarang pertanyaan</p>
          </div>
            
        </div>
