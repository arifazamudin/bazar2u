    <section class="site-section bg-light" >
      <div class="container">
        <h4>Permohonan lokasi baharu</h4>
        <div class="col-md-7 mb-5 site-animate">
          <form action="" method="post">
            <div class="form-group">
              <label for="name" class="sr-only">Name</label>
              <input type="text" class="form-control" id="name" placeholder="Nama">
            </div>
            <div class="form-group">
              <label for="email" class="sr-only">No Tel:</label>
              <input type="text" class="form-control" id="email" placeholder="No Tel:">
            </div>
            <div class="form-group">
              <label for="message" class="sr-only">Alamat</label>
              <textarea name="message" id="message" cols="30" rows="5" class="form-control" placeholder="Alamat"></textarea>
            </div>
            <div class="form-group">
              <input type="submit" class="btn btn-primary btn-lg" value="Mohon">
            </div>
          </form>
        </div>
        
        
      </div>
    </section>
