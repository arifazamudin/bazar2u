    <section class="site-section bg-light " >
        
          <div class="justify-content-center">
            
    
                <% products.forEach(function(p){ %>
                        <div class="card">
                            
                            <img class="pimage" src="https://bazar2you.s3.ap-southeast-1.amazonaws.com/product_images/<%= p.id %>/<%= p.image %> " style="width:auto" alt="">
                            
                            
                              <h3> <%= p.title %></h3>
                              <p> <%=p.desc %></p>
                              <p class="price">RM<%= parseFloat(p.price).toFixed(2) %></p>
                              <p ><a class="btn btn-primary" href="/cart/add/<%= p.slug %>">Add to Cart</a></p>

                            
                            
                        </div>
                    <% }); %>
                
                  </div> 
            
        
      </section>

   
